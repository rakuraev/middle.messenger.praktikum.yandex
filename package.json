{
  "name": "yandex.praktikum.messanger",
  "version": "0.0.1",
  "description": "Web-application \"chat\"",
  "scripts": {
    "dev": "parcel --port 3000",
    "build": "rimraf dist && parcel build index.html",
    "start:express": "cd server && node index.js",
    "lint": "eslint  src/ --ext .js --ext .jsx",
    "lint-fix": "npm run lint --fix",
    "start": "concurrently \"npm run build\" \"npm run start:express\""
  },
  "keywords": [
    "ya_praktikum",
    "messanger"
  ],
  "author": "Klyuev Nikita",
  "license": "ISC",
  "dependencies": {
    "express": "4.18.1",
    "handlebars": "4.7.7"
  },
  "devDependencies": {
    "@parcel/transformer-postcss": "2.6.2",
    "autoprefixer": "10.4.7",
    "babel-eslint": "10.1.0",
    "concurrently": "7.3.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "normalize.css": "8.0.1",
    "parcel": "2.6.2",
    "parcel-reporter-static-files-copy": "1.3.4",
    "parcel-transformer-hbs": "1.0.4",
    "postcss": "8.4.14",
    "postcss-css-variables": "0.18.0",
    "postcss-import": "14.1.0",
    "postcss-mixins": "9.0.3",
    "postcss-modules": "4.3.1",
    "postcss-nesting": "10.1.10",
    "prettier": "2.7.1",
    "reset-css": "5.0.1",
    "rimraf": "3.0.2"
  },
  "source": "index.html",
  "alias": {
    "variables": "./src/assets/styles/variables/"
  },
  "staticFiles": {
    "staticPath": "static",
    "staticOutPath": "static"
  }
}
